{
  "name": "@funnyzak/http-server",
  "version": "0.1.3",
  "description": "a static http server",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"o test specified\"",
    "dev": "watch \"npm run build\" src",
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/funnyzak/static-http-server.git"
  },
  "keywords": [
    "server",
    "static-http-server",
    "http",
    "nodejs"
  ],
  "author": "eric <silenceace@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/funnyzak/static-http-server/issues"
  },
  "homepage": "https://github.com/funnyzak/static-http-server#readme",
  "dependencies": {
    "chalk": "4.1.2",
    "mime": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "core-js": "3.19.1",
    "np": "*",
    "watch": "1.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.3.5"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "np": {
    "yarn": false,
    "contents": "./lib",
    "branch": "master",
    "releaseDraft": false
  },
  "publishConfig": {
    "access": "public"
  }
}
