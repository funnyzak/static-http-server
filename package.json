{
  "name": "@funnyzak/http-server",
  "version": "0.2.2",
  "description": "a static http server",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "ts3.1/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --fix src",
    "test": "echo \"o test specified\"",
    "dev": "watch \"npm run build\" src",
    "build": "tsc",
    "prepublish": "npm run build",
    "release": "np",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/funnyzak/static-http-server.git"
  },
  "keywords": [
    "server",
    "static-http-server",
    "http",
    "nodejs",
    "funnyzak"
  ],
  "author": "eric <silenceace@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/funnyzak/static-http-server/issues"
  },
  "homepage": "https://github.com/funnyzak/static-http-server#readme",
  "dependencies": {
    "chalk": "4.1.2",
    "mime": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "core-js": "3.19.1",
    "eslint": "8.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "np": "*",
    "prettier": "2.4.1",
    "typescript": "4.5.2",
    "watch": "1.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.3.5"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "np": {
    "yarn": false,
    "contents": ".",
    "branch": "master",
    "releaseDraft": false
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
