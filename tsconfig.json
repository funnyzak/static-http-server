{
  "compilerOptions": {
    "sourceMap": false,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "outDir": "./lib",
    "declaration": false
  },
  "include": [
    "./src/**/*"
, "lib/server.d.ts"  ]
}
